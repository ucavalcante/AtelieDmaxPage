    "artesanato",
    "mdf",
    "bordado eletrônico",
    "costura",
    "enxoval bebê",
    "convites personalizados",
    "typescript",
    "vue.js",
    "portfolio",
    "github-pages"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ucavalcante/AtelieDmaxPage/issues"
  },
  "homepage": "https://github.com/ucavalcante/AtelieDmaxPage#readme",
  "dependencies": {
    "bootstrap": "^4.4.1"
  },
  "devDependencies": {
