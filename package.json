{
  "name": "ateliedmaxpage",
  "version": "1.0.0",
  "description": "Portfolio website for Ateliê Dmax - artesanato em MDF, costura e bordados eletrônicos. Featuring TypeScript/Vue.js architecture with responsive design and auto-image detection.",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "check": "npx tsc --noEmit",
    "dev": "npm run build:watch",
    "build:clean": "find js/ -name '*.js' ! -name '.gitkeep' -delete && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucavalcante/AtelieDmaxPage.git"
  },
  "author": "Ulises Cavalcante",
